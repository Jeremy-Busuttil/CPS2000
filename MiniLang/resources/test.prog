//Iterative implementation of factorial
def fact(x)
{
  int product = 1;
  for (i=0; i<=x; i++)
  { 
    product = product * i; 
  }
  return product;
}

//A recursive implementation of fibonacci
def fibonacci(n)
{
  if (n <= 2)
  {
     return 1;
  }
  else
  {
     return fibonacci(n-1) + fibonacci(n-2);
  }
}

int y = fact(3);
int z = fibonacci(10);
int w = y + z;